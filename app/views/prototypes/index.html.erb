<main class="main">
  <div class="inner">
    <%# ログインしているときは以下を表示する %>
    <% if user_signed_in? %>
      <div class="greeting">
        こんにちは、
        <%# current_user %>
        <%= link_to "#{current_user.name}さん", user_path(current_user), class: :greeting__link%>
      </div>
    <% end %>
    <%# // ログインしているときは上記を表示する %>
    <%# 投稿機能実装後、部分テンプレートでプロトタイプ投稿一覧を表示する %>
    <%# collection: @prototypes→@prototypeにある要素を取得可能 %>
    <div class="card__wrapper">
      <%= render partial: 'prototype', collection: @prototype %>
    </div>
  </div>
</main>
